@model DressCodeViewModel
@{
    ViewData["Title"] = "Add New Dress Code";
}
<div class="col-6 mainBody" style="margin:auto">
    <h3>Add a Dress Code</h3>
    <form asp-controller="Resort" asp-action="AddDressCode" method="post" enctype="application/x-www-form-urlencoded">
        <partial name="_DressCodeAddEditPartial" />
        <br />
        <button type="submit" class="btn btn-primary">Add</button>   
        <a asp-controller="Resort" asp-action="GetResortInfo" class="btn btn-primary">Cancel</a>
    </form>
</div>
